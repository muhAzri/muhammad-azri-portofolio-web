---
export interface ServicesProps {
    name: string;
    description: string;
}

const { name, description } = Astro.props as ServicesProps;
---

<div>
    <div>
        <span class="text-white font-display text-xl">
            {name}:
        </span>
    </div>
    <p class="mt-3 text-eagle">
        {description}
    </p>
</div>
